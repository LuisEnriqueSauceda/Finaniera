<p id="notice"><%= notice %></p>

<h1>Listing Agents</h1>

<table>
  <thead>
    <tr>
      <th>Clave</th>
      <th>Nombre del agente</th>
      <th>Apellido materno</th>
      <th>Apellido paterno</th>
      <th>Nombre 1</th>
      <th>Nombre 2</th>
      <th>Rfc</th>
      <th>Calle</th>
      <th>Numero interior</th>
      <th>Numero exterior</th>
      <th>Colonia</th>
      <th>Codigo postal</th>
      <th>Municipio</th>
      <th>Estado</th>
      <th>Curp</th>
      <th>Seguro social</th>
      <th>Ine</th>
      <th>Nacionalidad</th>
      <th>Fecha de nacimiento</th>
      <th>Ciudad de nacimiento</th>
      <th>Estado de nacimiento</th>
      <th>Pais de nacimiento</th>
      <th>Sexo</th>
      <th>Telefono de casa</th>
      <th>Telefono celular</th>
      <th>Correo electronico 1</th>
      <th>Correo electronico 2</th>
      <th>Cuenta bancaria</th>
      <th>Banco</th>
      <th>Comision por cobranza</th>
      <th>Comision por colocacion</th>
      <th>Monto maximo de prestamo</th>
      <th>Permitir prestamo multiple</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @agents.each do |agent| %>
      <tr>
        <td><%= agent.clave %></td>
        <td><%= agent.nombre_del_agente %></td>
        <td><%= agent.apellido_materno %></td>
        <td><%= agent.apellido_paterno %></td>
        <td><%= agent.nombre_1 %></td>
        <td><%= agent.nombre_2 %></td>
        <td><%= agent.RFC %></td>
        <td><%= agent.calle %></td>
        <td><%= agent.numero_interior %></td>
        <td><%= agent.numero_exterior %></td>
        <td><%= agent.colonia %></td>
        <td><%= agent.codigo_postal %></td>
        <td><%= agent.municipio %></td>
        <td><%= agent.estado %></td>
        <td><%= agent.CURP %></td>
        <td><%= agent.seguro_social %></td>
        <td><%= agent.INE %></td>
        <td><%= agent.nacionalidad %></td>
        <td><%= agent.fecha_de_nacimiento %></td>
        <td><%= agent.ciudad_de_nacimiento %></td>
        <td><%= agent.estado_de_nacimiento %></td>
        <td><%= agent.pais_de_nacimiento %></td>
        <td><%= agent.sexo %></td>
        <td><%= agent.telefono_de_casa %></td>
        <td><%= agent.telefono_celular %></td>
        <td><%= agent.correo_electronico_1 %></td>
        <td><%= agent.correo_electronico_2 %></td>
        <td><%= agent.cuenta_bancaria %></td>
        <td><%= agent.banco %></td>
        <td><%= agent.comision_por_cobranza %></td>
        <td><%= agent.comision_por_colocacion %></td>
        <td><%= agent.monto_maximo_de_prestamo %></td>
        <td><%= agent.permitir_prestamo_multiple %></td>
        <td><%= link_to 'Show', agent %></td>
        <td><%= link_to 'Edit', edit_agent_path(agent) %></td>
        <td><%= link_to 'Destroy', agent, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

