<h1>Informacion de usuario</h1>

<%="nombre: #{@user.app} #{@user.apm} #{@user.nom1} #{@user.nom2}"%></br>
<%="correo: #{@user.email}"%></br>
<%="RFC: #{@user.RFC},  INE: #{@user.INE}, seguro social: #{@user.seguro}" %></br>
<%="Nacionalidad: #{@user.nacionalidad}"%></br>
<%="Fecha de nacimiento: #{@user.fecha_nacimiento}"%></br>
<%="Ciudad de nacimieto: #{@user.nac_ciudad}"%></br>
<%="Estado de nacimiento : #{@user.nac_estado}"%></br>
<%="Pais de nacimiento: #{@user.nac_pais}"%></br>
<%="sexo: #{ @user.sexo ? "Masculino" :"Femenino" }"%></br>
<%="tipo de usuario: #{
    case @user.tipo
        when 1
            nombre_tipo ="Administrador general"
        when 2
            nombre_tipo ="Asistente Major"
        when 3
            nombre_tipo ="Sucursal"
        when 4
            nombre_tipo ="Consultas"
    end
    nombre_tipo
}"%></br>
<%="Telefono: #{@user.telefono_casa}"%></br>
<%="Celular: #{@user.telefono_celular}"%></br>
<%="correo1: #{@user.correo1}"%></br>
<%="correo2: #{@user.correo2}"%></br>
<h5> Direccion:</h5>
<%="calle:  #{@user.calle} No. ext:#{@user.numero_int} #{"No.int: #{@user.numero_int}" unless @user.numero_int.nil? } CP: #{@user.codigo_postal}"%></br>
<%="Colonia: #{@user.colonia}"%></br>
<%="Municipio: #{@user.municipio}"%></br>
<%="Estado: #{@user.estado}"%></br>
<%="Pais: #{@user.pais}"%></br>



<%=link_to "Editar", edit_user_path%>